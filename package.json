    "typescript": "^5.2.2",
    "unocss": "^0.57.5",
    "vite": "^5.0.0",
    "vite-plugin-style-import": "^2.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "__bak_path": "./node_modules/cz-conventional-changelog",
      "path": "./node_modules/cz-customizable"
    }
  }
}
