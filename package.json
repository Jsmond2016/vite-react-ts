    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.1.0",
    "mrm": "^4.1.22",
    "msw": "^2.0.8",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylus": "^0.62.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.5",
    "vite": "^5.0.0",
    "vite-plugin-style-import": "^2.0.0"
  },
  "msw": {
    "workerDirectory": "public"
