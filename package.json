{
  "name": "react18-demo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc && vite build",
    "genCodes": "ts-node --project tsconfig.node.json ./src/tools/genCode.ts",
    "preview": "vite preview",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "commit": "git cz",
    "lint:fix": "eslint --cache ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore",
    "__mrm 配置__": "如下",
    "__lint__bak": "eslint . --cache --fix --ext .ts,.tsx",
    "__format_ba": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "__posttest_bak": "npm run format",
    "__eslint__refer": "eslint 配置来源:=> https://github.com/TheSwordBreaker/vite-reactts-eslint-prettier/blob/main/package.json"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "ahooks": "^3.7.8",
    "antd": "^5.11.2",
    "axios": "^1.6.2",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.10",
    "highlight.js": "^11.9.0",
    "jotai": "^2.8.0",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-rnd": "^10.4.1",
    "react-router-dom": "^6.19.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/eslint": "^8.44.9",
    "@types/node": "^20.9.2",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-rnd": "^8.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@umijs/fabric": "^4.0.1",
    "@unocss/preset-attributify": "^0.57.5",
    "@unocss/preset-icons": "^0.57.5",
    "@unocss/preset-uno": "^0.57.5",
    "@unocss/transformer-attributify-jsx": "^0.57.6",
    "@vitejs/plugin-react": "^4.2.0",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.1.0",
    "mockjs": "^1.1.0",
    "mrm": "^4.1.22",
    "msw": "^2.0.8",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylus": "^0.62.0",
